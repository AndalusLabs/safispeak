{
  "expo": {
    "name": "safispeak",
    "slug": "safispeak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_new_black.png",
    "scheme": "safispeak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
  "image": "./assets/images/splash-icon.png",
  "resizeMode": "contain",
  "backgroundColor": "#fff9e9"
},
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_new_black.png",
        "backgroundColor": "#fff9e9"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 260,
          "resizeMode": "contain",
          "backgroundColor": "#fff9e9"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
